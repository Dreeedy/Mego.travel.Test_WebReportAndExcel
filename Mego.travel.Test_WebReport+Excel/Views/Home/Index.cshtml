@model IEnumerable<Mego.travel.Test_WebReport_Excel.Models.Order>
@{
    ViewData["Title"] = "Заказы";
}

<div class="text-center">
    <div>
        <h3>Заказы</h3>
        <div>
            
        </div>
        <table class="table">
            <thead class="thead-light">
                <tr>
                    <th>Идентификатор</th>
                    <th>Сумма заказа</th>
                    <th>Дата заказа</th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var order in Model)
                {
                    <tr>
                        <td>@order.Id</td>
                        <td>@order.Price</td>
                        <td>@order.Date</td>
                        <td><a href="~/Home/Update/@order.Id">Редактировать</a></td>
                        <td><a href="~/Home/Delete/@order.Id">Удалить</a></td>
                    </tr>
                }
            </tbody>            
        </table>
    </div>
</div>
